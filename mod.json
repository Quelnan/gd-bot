{
    "geode": "3.0.0",
    "gd": {
        "win": "2.206",
        "android": "2.206",
        "mac": "2.206"
    },
    "id": "your.name.gdpathfinder",
    "name": "GD Pathfinder",
    "version": "v1.0.0",
    "developer": "Quelnan",
    "description": "Automatic level pathfinder with GD physics simulation",
    "repository": "https://github.com/yourusername/gd-pathfinder",
    "dependencies": [],
    "resources": {
        "files": []
    },
    "settings": {
        "beam-width": {
            "name": "Beam Width",
            "description": "Number of states to keep during search (higher = slower but more accurate)",
            "type": "int",
            "default": 5000,
            "min": 100,
            "max": 50000
        },
        "log-level": {
            "name": "Log Level",
            "description": "Logging verbosity",
            "type": "string",
            "default": "info",
            "one-of": ["debug", "info", "warn", "error"]
        }
    }
}
